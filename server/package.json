{
  "name": "react-node-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --poll --respawn --transpile-only src/index.ts",
    "format": "prettier --write src/",
    "lint": "eslint src/",
    "typeorm": "typeorm-ts-node-commonjs",
    "show-migrations": "npm run typeorm -- migration:show -d ./src/data-source.ts",
    "run-migrations": "npm run typeorm -- migration:run -d ./src/data-source.ts",
    "revert-migration": "npm run typeorm -- migration:revert -d ./src/data-source.ts",
    "new-migration": "npm run typeorm migration:create ./src/migrations/migration"
  },
  "keywords": [],
  "author": "Konrad Perlicki <konrad.perlicki01@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pg": "^8.8.0",
    "redis": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.11.0",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "prettier": "2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
